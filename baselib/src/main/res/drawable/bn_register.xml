<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/bn_corner"/>
            <solid android:color="@color/bn_login"/>
        </shape>
    </item>

    <item>
        <shape>
            <corners android:radius="@dimen/bn_corner"/>
            <solid android:color="@color/bn_register"/>
            <stroke android:width="@dimen/bn_stroke" android:color="#FF4bd863"/>
        </shape>
    </item>

</selector>